FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}
offCentre 0.6;

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
}


divSchemes
{
    default         none;
    div(phi,T)      Gauss linearUpwind default; //vanLeer01; // linear;
}

laplacianSchemes
{
    default         Gauss linear orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}


